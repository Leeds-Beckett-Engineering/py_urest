@startuml urest_api
!theme vibrant
namespace urest {
    namespace api {
        namespace base <<Rectangle>> {
            abstract class APIBase {
                - _state_attributes : dict
                {abstract} + delete_state()
                {abstract} + get_state()
                {abstract} + set_state()
                {abstract} + update_state()
            }
        }

    }
}

@enduml
